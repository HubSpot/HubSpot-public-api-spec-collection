            }
          },
          "default" : {
            "$ref" : "#/components/responses/Error"
          }
        },
        "security" : [ {
          "private_apps" : [ "crm.export" ]
        }, {
          "oauth2" : [ "crm.export" ]
        } ]
      }
    },
