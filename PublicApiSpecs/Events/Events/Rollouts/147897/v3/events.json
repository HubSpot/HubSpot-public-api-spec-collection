  "paths" : {
    "/events/v3/events/" : {
      "get" : {
        "tags" : [ "Events" ],
        "summary" : "Retrieve event occurrences",
        "description" : "Retrieve occurrences of events. For example, retrieve all event occurrences associated with a specific contact.",
        "operationId" : "get-/events/v3/events/_getPage",
        "parameters" : [ {
          "name" : "after",
          "in" : "query",
          "description" : "The paging cursor token of the last successfully read resource will be returned as the `paging.next.after` JSON property of a paged response containing more results.",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "before",
          "in" : "query",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "eventType",
          "in" : "query",
          "description" : "The event type name. You can retrieve available event types using the [event types endpoint](#get-%2Fevents%2Fv3%2Fevents%2Fevent-types).",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "id",
          "in" : "query",
          "description" : "ID of an event instance. IDs are 1:1 with event instances. If you provide this filter and additional filters, the other filters must match the values on the event instance to yield results.",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          }
        }, {
          "name" : "limit",
          "in" : "query",
          "description" : "The maximum number of results to display",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int32"
          }
        }, {
          "name" : "objectId",
          "in" : "query",
          "description" : "The ID of the CRM Object to filter event instances on. When including this parameter, you must also include the `objectType` parameter.",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "integer",
            "format" : "int64"
          }
        }, {
          "name" : "objectProperty.{propname}",
          "in" : "query",
          "description" : "Instead of retrieving event data for a specific object by its ID, you can specify a unique identifier property. For contacts, you can use the `email` property. (e.g., `objectProperty.email=name@domain.com`).",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "object"
          }
        }, {
          "name" : "objectType",
          "in" : "query",
          "description" : "The type of CRM object to filter event instances on (e.g., `contact`). To retrieve event data for a specific CRM record, include the additional `objectId` query parameter (below). ",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "occurredAfter",
          "in" : "query",
          "description" : "Filter for event data that occurred after a specific datetime.",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string",
            "format" : "date-time"
          }
        }, {
          "name" : "occurredBefore",
          "in" : "query",
          "description" : "Filter for event data that occurred before a specific datetime.",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "string",
            "format" : "date-time"
          }
        }, {
          "name" : "property.{propname}",
          "in" : "query",
          "description" : "Filter for event completions that contain a specific value for an event property (e.g., `property.hs_city=portland`). For properties values with spaces, replaces spaces with `%20` or `+` (e.g., `property.hs_city=new+york`).",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "object"
          }
        }, {
          "name" : "sort",
          "in" : "query",
          "description" : "Sort direction (ascending or descending) based on the occurredAt timestamp of the event instance. To set the sort direction to ascending, pass `occurredAt`. To sort descending, add a minus prefix, like this: `-occurredAT`",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/CollectionResponseExternalUnifiedEvent"
